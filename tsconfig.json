{
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictNullChecks": true, 
        "target": "ES2020",
        "moduleResolution": "Node10",
        "module": "CommonJS",
        "declaration": true,
        "isolatedModules": true,
        "noEmit": true,
        "outDir": "./dist",
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}